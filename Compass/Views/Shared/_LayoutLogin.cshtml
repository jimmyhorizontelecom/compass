

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>HPSEDC | Login</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link href="~/PageAssets/css/newlogin.css" rel="stylesheet" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link href="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css" rel="stylesheet">
    <link href="~/ResCdn/cdn/css/bootstrap5.3.3.min" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

</head>
<body class="d-flex flex-column vh-100">

    <div class="row">
        <nav class="navbar header">
            <div class="container-fluid ms-md-5 d-flex justify-content-sm-center justify-content-lg-start justify-content-md-center justify-content-center">
                <a class="navbar-brand" href="#">
                    <img src="~/assets/images/login/newlogo.png"
                         alt="HPSEDC Logo"
                         class="img-fluid"
                         style="max-height: 100px;">
                </a>
            </div>
        </nav>
    </div>
    @*<div id="main" class="flex-grow-1 d-flex align-items-center justify-content-center justify-content-lg-end">*@
    <div id="main" class="flex-grow-1 d-flex align-items-center ">
        <header>
        </header>
        <main class="flex-grow-1">
            @RenderBody()
        </main>
    </div>
    <footer class="bg-white pt-1 footer">
        <div class="row d-flex g-0 justify-content-center">
            <div class="col-md-12  text-center">
                <p class="font montserrat">Copyright &copy; @DateTime.Now.Year Himachal Pradesh State Electronics Development Corporation Ltd.(HPSEDC).      All Rights Reserved</p>
            </div>
            
        </div>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    @* <script src="@siteUrl/Scripts/browser.js"></script> *@
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
